<div class="booyah-box col-10 offset-1">
  <h1>Add Project Details</h1>
  <%= simple_form_for @project do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :plan, collection: ["Beta", "MVP", "Startup"]%>
    <%= f.input :planhours %>
    <%= f.input :thours %>
</br> 
    <%= f.submit 'Add', class: 'btn btn-primary' %>
  <% end %>
</div>